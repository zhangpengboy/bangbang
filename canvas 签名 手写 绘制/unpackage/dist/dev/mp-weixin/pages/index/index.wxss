
.title{
	height:50rpx;
	line-height: 50rpx;
	font-size: 16px;
}
.mycanvas{
	width: 100%;
	height: calc(100vh - 200rpx);
	background-color: #ECECEC;
}
.footer{
	font-size: 16px;
	height: 150rpx;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.left,.right{
	line-height: 100rpx;
	height: 100rpx;
	width: 250rpx;
	text-align: center;
	font-weight: bold;
	color: white;
	border-radius: 5rpx;
}
.left{
	background: #007AFF;
}
.right{
	background:orange;
}

